{
    "name": "kodekeep/laravel-reviews",
    "description": "Attach Reviews to Laravel Eloquent Models.",
    "keywords": [
        "kodekeep",
        "laravel-reviews"
    ],
    "homepage": "https://github.com/kodekeep/laravel-reviews",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@kodekeep.com",
            "homepage": "https://kodekeep.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^6.0",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "graham-campbell/analyzer": "^2.4",
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.3",
        "spatie/phpunit-snapshot-assertions": "^3.1",
        "symfony/var-dumper": "4.4"
    },
    "autoload": {
        "psr-4": {
            "KodeKeep\\Reviews\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KodeKeep\\Reviews\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html=.coverage --coverage-clover=clover.xml",
        "format": "vendor/bin/php-cs-fixer fix ./"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "KodeKeep\\Reviews\\Providers\\ReviewsServiceProvider"
            ],
            "aliases": {
                "Reviews": "KodeKeep\\Reviews\\ReviewsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
